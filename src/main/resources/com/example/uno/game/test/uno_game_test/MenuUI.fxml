<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="690.0" prefWidth="900.0" spacing="20" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.uno.game.test.uno_game_test.MenuController">
   <Label text="UNO">
      <font>
         <Font name="Arial Black" size="125.0" />
      </font>
      <VBox.margin>
         <Insets bottom="150.0" />
      </VBox.margin>
   </Label>
    <Button fx:id="singleplayerButton" onAction="#handleSingleplayer" prefWidth="300.0" text="Singleplayer">
      <VBox.margin>
         <Insets />
      </VBox.margin>
      <padding>
         <Insets bottom="10.0" top="10.0" />
      </padding></Button>
    <Button fx:id="multiplayerButton" onAction="#handleMultiplayer" prefWidth="300.0" text="Multiplayer">
      <padding>
         <Insets bottom="10.0" top="10.0" />
      </padding></Button>
</VBox>
